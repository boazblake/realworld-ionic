{
  "name": "realworld-ionic",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "clean": "npm cache verify && rm -rf ./docs && rm -rf ./node_modules",
    "restart": "rm -rf docs && npm run start",
    "fresh": "npm run clean && npm install && npm run start",
    "debug": "export LOGGY_STACKS=1 && npm run start",
    "start": "brunch watch --server",
    "ionic:build": "npm run build",
    "build:pre": "rm -rf docs",
    "build": "npm run build:pre && brunch build --production",
    "gitcommit": "git add .; git commit -m",
    "gitbuild": "git add .; git commit -m 'build'; git push",
    "sync": "ionic cap sync"
  },
  "dependencies": {
    "data.task": "^3.1.2",
    "marked": "^1.1.0",
    "mithril": "^2.0.4",
    "ramda": "^0.27.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@capacitor/android": "2.4.1",
    "@capacitor/core": "2.4.1",
    "@capacitor/ios": "2.4.1",
    "@ionic-native/camera": "^5.30.0",
    "@ionic-native/core": "^5.30.0",
    "@ionic/core": "^5.5.2",
    "uglify-js-brunch": "^2.10.0",
    "auto-reload-brunch": "git+https://github.com/brunch/auto-reload-brunch.git",
    "babel-brunch": "7.0.1",
    "brunch": "3.0.0",
    "clean-css-brunch": "3.0.0",
    "copycat-brunch": "1.1.1",
    "imagemin-brunch": "^1.1.0",
    "sass-brunch": "3.0.0",
    "size-limit": "^4.9.1",
    "sw-precache-brunch": "^2.0.1",
    "terser-brunch": "^4.0.0"
  }
}
